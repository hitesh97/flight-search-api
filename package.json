{
  "name": "flight-search-api",
  "version": "1.0.0",
  "description": "Flight search API with CO2 emission calculation using Node.js, GraphQL, and TypeScript.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint . --ext .ts",
    "prebuild": "rimraf dist",
    "test": "jest"
  },
  "dependencies": {
    "@types/ioredis-mock": "^8.2.5",
    "apollo-server-express": "^3.13.0",
    "axios": "^1.7.7",
    "dotenv": "^8.2.0",
    "express": "^4.21.0",
    "express-graphql": "^0.12.0",
    "graphql": "^16.9.0",
    "graphql-tools": "^9.0.1",
    "ioredis": "^5.4.1",
    "ioredis-mock": "^8.9.0",
    "pg": "^8.7.1",
    "redis": "^4.7.0",
    "redis-mock": "^0.56.3",
    "ts-jest": "^29.2.5"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/express": "^5.0.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^14.14.31",
    "@types/pg": "^8.11.10",
    "@types/redis": "^4.0.11",
    "@types/redis-mock": "^0.17.3",
    "eslint": "^7.21.0",
    "jest": "^29.7.0",
    "nodemon": "^2.0.7",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^4.2.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
